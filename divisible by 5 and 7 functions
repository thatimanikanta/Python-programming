def my_function(n):
    if n%5==0 and n%7==0:
        return "divisible by both 5 and 7"
    elif n%5==0:
        return "divisible by only 5"
    elif n%7==0:
        return "divisible by only 7"
    else:
        return "not divisible by 5 and 7"
n=int(input("enter the number:"))
print(my_function(n))
