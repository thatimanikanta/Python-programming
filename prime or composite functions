def my_function(n,count):
    if n==0 or n==1:
        return "neither prime nor composite"
    else:
        if n>=2:
            for i in range(2,n):
                if n%i==0:
                    count+=1
    if count==0:
        return "prime number"
    else:
        return "composite number"
n=int(input("enter the number:"))
count=0
print(my_function(n,count))
